	<!-- NORMALIZE LOOT IN ALL BIOMES -->
	
	<set xpath="//biome[@name='pine_forest']/@lootstage_bonus">30</set>
	<set xpath="//biome[@name='desert']/@lootstage_bonus">30</set>
	<set xpath="//biome[@name='snow']/@lootstage_bonus">30</set>
